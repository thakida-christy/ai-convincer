 {
      "version": 2,
      "builds": [
        {
          "src": "imagegenerator/wsgi.py",
          "use": "@vercel/python",
          "config": {"maxLambdaSize": "15mb", "runtime": "python3.13.3"}
        },
        {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": { "distDir": "static" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "imagegenerator/wsgi.py"
        }
      ]
    }
